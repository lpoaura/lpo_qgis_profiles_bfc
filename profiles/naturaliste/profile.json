{
  "$schema": "https://raw.githubusercontent.com/Guts/qgis-deployment-cli/main/docs/schemas/profile/qgis_profile.json",
  "name": "naturaliste",
  "folder_name": "naturaliste",
  "description": "QGIS profile pour les chargé.e.s de mission Naturaliste à la LPO.",
  "author": "Pôle système d'information",
  "email": "aura.support@lpo.fr",
  "icon": "images/logo_qgis_lpo.ico",
  "qgisMinimumVersion": "3.28.0",
  "qgisMaximumVersion": "3.99.10",
  "version": "1.0.2",
  "plugins": [
    {
      "name": "Bezier Editing",
      "folder_name": "BezierEditing",
      "official_repository": true,
      "plugin_id": 1719,
      "version": "1.3.8"
    },
    {
      "name": "French Locator Filter",
      "folder_name": "french_locator_filter",
      "official_repository": true,
      "plugin_id": 1846,
      "version": "1.1.1"
    },
    {
      "name": "Group Stats",
      "folder_name": "GroupStats",
      "official_repository": true,
      "plugin_id": 40,
      "version": "2.2.7"
    },
    {
      "name": "QField Sync",
      "folder_name": "qfieldsync",
      "official_repository": true,
      "plugin_id": 1083,
      "version": "v4.9.1"
    },
    {
      "name": "Mask",
      "folder_name": "mask",
      "official_repository": true,
      "plugin_id": 65,
      "version": "1.10.2"
    },
    {
      "name": "Plugin LPO",
      "folder_name": "plugin_qgis_lpo",
      "version": "3.3.1",
      "repository_url_xml": "https://github.com/lpoaura/PluginQGis-LPOData/releases/download/3.3.1/plugins.xml",
      "official_repository": false
    },
    {
      "name": "Layers menu from project",
      "folder_name": "menu_from_project",
      "official_repository": true,
      "plugin_id": 43,
      "version": "2.1.0"
    },
    {
      "name": "Spreadsheet Layers",
      "folder_name": "SpreadsheetLayers",
      "official_repository": true,
      "plugin_id": 766,
      "version": "2.1.2"
    }
  ],
  "rules": [
    {
      "name": "Domaine",
      "description": "Restriction de diffusion sur les groupes AD ou OS",
      "conditions": {
        "any": [
          {
            "path": "$.user.groups_domain",
            "value": "GG-GPOqgis_01",
            "operator": "contains"
          },
          {
            "path": "$.user.groups_domain",
            "value": "GG-GPOqgis_07",
            "operator": "contains"
          },
          {
            "path": "$.user.groups_domain",
            "value": "GG-GPOqgis_07",
            "operator": "contains"
          },
          {
            "path": "$.user.groups_domain",
            "value": "GG-GPOqgis_26",
            "operator": "contains"
          },
          {
            "path": "$.user.groups_domain",
            "value": "GG-GPOqgis_38",
            "operator": "contains"
          },
          {
            "path": "$.user.groups_domain",
            "value": "GG-GPOqgis_42",
            "operator": "contains"
          },
          {
            "path": "$.user.groups_domain",
            "value": "GG-GPOqgis_63",
            "operator": "contains"
          },
          {
            "path": "$.user.groups_domain",
            "value": "GG-GPOqgis_69",
            "operator": "contains"
          },
          {
            "path": "$.user.groups_domain",
            "value": "GG-GPOqgis_73",
            "operator": "contains"
          },
          {
            "path": "$.user.groups_domain",
            "value": "GG-GPOqgis_74",
            "operator": "contains"
          },
          {
            "path": "$.environment.operating_system_code",
            "value": "linux",
            "operator": "equal"
          }
        ]
      }
    }
  ]
}